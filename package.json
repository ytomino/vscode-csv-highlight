{
  "name": "csv-highlight",
  "displayName": "CSV Highlight",
  "description": "CSV syntax highlight",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "csv",
      "aliases": ["CSV", "csv"],
      "extensions": [".csv"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "csv",
      "scopeName": "source.csv",
      "path": "./syntaxes/csv.tmLanguage.json"
    }]
  }
}
